<nav class="navbar navbar-inverse" role="navigation">
      <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse-01">
        <span class="sr-only">Toggle navigation</span>
      </button>
      <a class="navbar-brand" href="/">SERVICE NAME</a>
    </div>
    <div class="collapse navbar-collapse" id="navbar-collapse-01">
      <p class="navbar-text navbar-right">로그인이 필요합니다.</p>
    </div><!-- /.navbar-collapse -->
</nav>
<main>
  <div class="valign-wrapper">
    <div class="login valign">
      <h2>학생 로그인</h2>
      <% flash.each do |key, value| %>
      <blockquote>
        <p>아래 메시지를 확인하여 주세요.</p>
        <small><%= value %></small>
      </blockquote>
      <% end %>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name), :html => {:class => "login-form"}) do |f| %>
      <div class="form-group">
        <%= f.label :email, "이메일" %><br />
        <%= f.email_field :email, autofocus: true, class:"form-control" %>
      </div>
    
      <div class="form-group">
        <%= f.label :password, "패스워드" %><br />
        <%= f.password_field :password, autocomplete: "off", class:"form-control" %>
      </div>
    
      <% if devise_mapping.rememberable? -%>
        <div class="form-group">
          <%= f.check_box :remember_me %>
          <%= f.label :remember_me %>
        </div>
      <% end -%>
    
      <div class="actions">
        <%= f.submit "로그인", class:"btn btn-primary btn-lg btn-block btn-embossed" %>
      </div>
    <% end %>
    
    <%= render "users/shared/links" %>
      </div>
  </div>
</main>